<Window x:Class="OptiekProject_WPF.SterkteBeheren"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OptiekProject_WPF"
        mc:Ignorable="d"
        Title="SterkteBeheren" Height="225" Width="400">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel HorizontalAlignment="Left" Height="380" Width="130">
                <TextBox Height="35" Width="110" Margin="10"/>
                <TextBox Height="35" Width="110" Margin="10"/>
                <Button Content="aanmaken" Height="70" Width="120" Margin="5,10,5,10" Command="{Binding}" CommandParameter="SterkteAanmaken"></Button>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
